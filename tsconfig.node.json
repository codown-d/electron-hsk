{
    "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
    "include": [
        "src/renderer/src/env.d.ts",
        "electron.vite.config.*",
        "src/main/**/*",
        "src/preload/**/*",
        "src/lowdb/**/*"
    ],
    "compilerOptions": {
        "composite": true,
        "allowImportingTsExtensions": true,
        "noEmit": true,
        "types": [
            "electron-vite/node"
        ]
    }
}